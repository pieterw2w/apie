{
    "name": "w2w/apie",
    "description": "apie is a library to convert plain old php objects into a REST API with OpenAPI spec.",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Pieter Jordaan",
            "email": "pieter.jordaan@way2web.nl"
        }
    ],
    "require": {
        "php": "^7.2",
        "symfony/serializer": "^4.2|^5.0",
        "psr/cache": "^1.0",
        "symfony/cache": "^4.0|^5.0",
        "symfony/http-kernel": "^4.0|^5.0",
        "symfony/property-access": "^4.0|^5.0",
        "symfony/property-info": "^4.0|^5.0",
        "doctrine/annotations": "^1.6",
        "ramsey/uuid": "^3.8",
        "erasys/openapi-php": "^2.1",
        "psr/http-message": "^1.0",
        "zendframework/zend-diactoros": "^2.1",
        "jeroen/rewindable-generator": "^1.2",
        "psr/container": "^1.0",
        "gbprod/uuid-normalizer": "^1.1",
        "symfony/finder": "^4.3",
        "phpdocumentor/reflection-docblock": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "W2w\\Lib\\Apie\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "W2w\\Test\\Apie\\": "tests/"
        }
    },
    "require-dev": {
        "nesbot/carbon": "*",
        "phpunit/phpunit": "^7.0.0|^8.0.0",
        "guzzlehttp/guzzle": "^6.3",
        "haydenpierce/class-finder": "^0.4.0",
        "webonyx/graphql-php": "^0.13.8",
        "phpstan/phpstan": "^0.12.3",
        "doctrine/cache": "^1.9",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan-phpunit": "0.12.2",
        "jangregor/phpstan-prophecy": "0.5.1"
    },
    "conflict": {
        "phpspec/prophecy": "1.10.*"
    },
    "archive": {
        "exclude": ["/tests"]
    }
}
